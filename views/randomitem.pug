extends layout

block content
  h1= title
  - var image = ["image.webp", "image2.jpg", "image3.avif", "image4.jpg", "image5.webp"]
  - for (let i = 0; i < 3; i++)
    - var random = Math.floor(Math.random()* 5)
    - var selected_image = image[random]
    img(src=`/images/${selected_image}`, alt=`Random Image ${i+1}`, height="300", width="300")